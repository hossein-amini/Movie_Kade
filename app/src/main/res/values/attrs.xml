<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableTextView">
        <attr name="text" format="string" />
        <attr name="maxLines" format="integer" />
        <attr name="button_textPrimary" format="string" />
        <attr name="button_textSecondary" format="string" />
        <attr name="buttonColor" format="color" />
    </declare-styleable>
    <declare-styleable name="DotIndicator">
        <attr name="activeColor" format="color" />
        <attr name="deactivateColor" format="color" />
        <attr name="dotCount" format="integer" />
        <attr name="dotSize" format="integer" />
        <attr name="activeDot" format="integer" />
        <attr name="dotMargin" format="integer" />
    </declare-styleable>
</resources>